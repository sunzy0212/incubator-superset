{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "less": false,
  "publicPath": "/",
  "autoprefixer": null,
  "extraBabelPlugins": [
    "transform-runtime",
	["import", { "libraryName": "antd", "style": "css" }]
  ],
  "env": {
    "development": {
      "proxy": {
         "/v1/*": {
            "target": "http://localhost:8000/",
            "changeOrigin": true,
            "pathRewrite": { "^/v1" : "/v1" }
          }
      },
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}
